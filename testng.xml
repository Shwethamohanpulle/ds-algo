<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DSPortal Heroku Application Test Automation Test Suite ">

<listeners>
		 <listener class-name="com.dsalgo.qa.ExtentReportListener.ExtentReporterNG"/>
        <listener class-name="com.dsalgo.qa.ExtentReportListener.AllureListeners"/>
    <listener class-name="com.dsalgo.qa.analyzer.AnnotationTransformer"/>
	</listeners>  

<test   name="DSPortal Heroku Application linkedlist Test Cases">

<parameter name="browserName" value="chrome"/>

 <classes>
      <class name="com.dsalgo.qa.testcases.LinkedListTest"/>
    </classes>
  </test>  




<test   name="DSPortal Heroku Application tree Test Cases ">

<parameter name="browserName" value="fireFox"/>

 <classes>
      <class name="com.dsalgo.qa.testcases.TreeTest"/>
    </classes>
  </test>


 <test name="DSPortal Heroku Application Data Structue Test Cases">
 <classes>
      <class name="com.dsalgo.qa.testcases.DataStructurePageTest"/>
    </classes>

</test>


